    "file-loader": "^1.1.5",
    "graphqelm": "3.1.12",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.6.2",
    "webpack": "4.8.3",
